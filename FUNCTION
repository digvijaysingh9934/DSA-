#include <iostream>
#include <cstdio>
#include <cmath>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int max_of_four(int a, int b, int c, int d)
{
    if(max(a,b)>=max(c,d))
    return max(a,b);
    else return max(c,d);
    }
   


int main() {
    int a, b, c, d;
    cin>>a>>b>>c>>d;
    int ans = max_of_four(a, b, c, d);
    cout<<ans;
    
    return 0;
}
